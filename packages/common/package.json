{
  "name": "@lace/common",
  "version": "0.1.0",
  "description": "Generic application elements common to @lace/cardano and @lace/core",
  "homepage": "https://github.com/input-output-hk/lace/blob/master/packages/core/README.md",
  "bugs": {
    "url": "https://github.com/input-output-hk/lace/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/input-output-hk/lace.git"
  },
  "license": "Apache-2.0",
  "author": "IOHK",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "files": [
    "dist",
    "LICENSE",
    "NOTICE",
    "README.md"
  ],
  "scripts": {
    "build": "run -T rollup -c rollup.config.js",
    "cleanup": "yarn exec rm -rf dist node_modules .rollup.cache src/dist",
    "lint": "cd ../.. && yarn common:lint",
    "prepack": "yarn build",
    "prepare": "ts-patch install -s",
    "prestart": "yarn build",
    "start": "node dist/index.js",
    "test": "NODE_ENV=test run -T jest -c ./test/jest.config.js",
    "watch": "yarn build --watch"
  },
  "dependencies": {
    "@cardano-sdk/util": "0.15.0",
    "antd": "^4.24.10",
    "classnames": "^2.3.1",
    "jdenticon": "3.1.0",
    "lodash": "4.17.21",
    "qr-code-styling": "1.6.0-rc.1",
    "react": "17.0.2",
    "react-copy-to-clipboard": "5.0.4",
    "react-dom": "17.0.2",
    "react-tooltip": "4.2.7"
  },
  "devDependencies": {
    "typescript": "^4.3.5"
  },
  "peerDependencies": {
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "5.2.0"
  }
}
