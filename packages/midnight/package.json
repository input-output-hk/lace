{
  "name": "@lace/midnight",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://github.com/input-output-hk/lace/blob/master/packages/midnight/README.md",
  "bugs": {
    "url": "https://github.com/input-output-hk/lace/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/input-output-hk/lace.git"
  },
  "license": "Apache-2.0",
  "author": "IOG",
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "typings": "./dist/index.d.ts",
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "files": [
    "dist",
    "LICENSE",
    "NOTICE",
    "README.md"
  ],
  "scripts": {
    "build": "npx rollup -c rollup.config.js",
    "cleanup": "shx rm -rf dist node_modules coverage reports .rollup.cache src/dist",
    "format-check": "echo \"@lace/midnight: no format-check command specified\"",
    "lint": "cd ../.. && yarn midnight:lint",
    "prepack": "yarn build",
    "prepare": "ts-patch install -s",
    "prestart": "yarn build",
    "test:coverage": "yarn test --coverage",
    "type-check": "echo \"@lace/midnight: no type-check command specified\"",
    "watch": "yarn build --watch"
  },
  "dependencies": {
    "@cardano-sdk/key-management": "0.15.1",
    "@midnight-ntwrk/wallet": "^3.5.9",
    "@midnight-ntwrk/wallet-api": "^3.3.1",
    "@midnight-ntwrk/zswap": "^0.3.11"
  },
  "devDependencies": {
    "shx": "^0.3.2",
    "typescript": "^4.3.5"
  },
  "peerDependencies": {
    "rxjs": "7.4.0",
    "zustand": "3.5.14"
