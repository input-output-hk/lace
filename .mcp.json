{
  "mcpServers": {
    "sequential-thinking": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-sequential-thinking"],
      "env": {}
    },
    "context7": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "@upstash/context7-mcp"],
      "env": {}
    },
    "interactive": {
      "command": "npx",
      "args": [
        "-y",
        "interactive-mcp",
        "-t",
        "3600",
        "--disable-tools",
        "message_complete_notification"
      ]
    }
  }
}
